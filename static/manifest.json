{
	"name": "Super Tab Manager",
	"description": "Most powerful browser tab manager ever created. Contact: wentallout@gmail.com",
	"version": "1.0.7",
	"permissions": [
		"tabs",
		"activeTab",
		"tabGroups",
		"favicon",
		"bookmarks",
		"system.memory",
		"scripting",
		"<all_urls>"
	],
	"host_permissions": ["<all_urls>"],
	"manifest_version": 3,
	"action": {
		"default_title": "Super Tab Manager - Open Menu",
		"default_icon": "icon128.png",
		"default_popup": "index.html"
	},

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"options_page": "options.html"
}
